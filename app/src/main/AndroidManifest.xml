<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quicksurvey">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dashboard" />
        <activity android:name=".allsurveys" />
        <activity android:name=".todaysurveys" />
        <activity android:name=".calendar" />
        <activity
            android:name=".seeresults"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".mysurveys" />
        <activity
            android:name=".pastresponse"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".pastSurveys" />
        <activity android:name=".attemptSurvey" />
        <activity android:name=".notifications2" />
        <activity android:name=".notifications" />
        <activity
            android:name=".Admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".settings" />
        <activity android:name=".submitTo" />
        <activity android:name=".createSurvey" />
        <activity
            android:name=".User"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>